{
  "name": "react-styleguide-generator-example",
  "private": true,
  "scripts": {
    "start": "npm run styleguide & npm run server",
    "styleguide": "rsg 'components/**/*.js' -w",
    "server": "browser-sync start --server ./styleguide --server ./app --files './styleguide/src/*'"
  },
  "dependencies": {
    "browser-sync": "^2.9.7",
    "react": "^0.13.3",
    "react-bootstrap": "^0.23.5"
  },
  "devDependencies": {
    "react-styleguide-generator": "*",
    "serve": "^1.4.0"
  }
}
